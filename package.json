{
  "name": "typescript-lambda-starter",
  "version": "1.0.0",
  "description": "Starter template for TypeScript Lambda functions",
  "main": "index.js",
  "repository": "https://github.com/dvonlehman/typescript-lambda-starter",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "tsc --noEmit && npm run validate-formatting && npm run lint && jest",
    "runlocal": "node scripts/runlocal.js",
    "fix-formatting": "prettier --write \"src/**/*.ts\"",
    "validate-formatting": "prettier --list-different \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "aws-sdk": "^2.493.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "yargs": "^13.3.0"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "winston": "^3.2.1",
    "winston-sentry-raven-transport": "^1.0.3"
  }
}
